{
  "name": "bankdata-links",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "concurrently -n emulators,frontend \"npm run emulators:start\" \"npm run start\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/bankdata-links/stats.json",
    "test": "ng test",
    "lint": "eslint --ext .ts src/",
    "functions:build": "cd functions && npm run build",
    "emulators:start": "npm run functions:build && firebase emulators:start --only firestore,functions,auth --export-on-exit=./saved-data --import=./saved-data --project=default",
    "emulators:force-kill": "npx kill-port --port 8080,9099,5001"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/router": "^15.2.2",
    "dom-to-json": "^0.0.4",
    "firebase": "^9.18.0",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "webpack": "5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.2",
    "@angular/compiler-cli": "^15.2.2",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.183",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
